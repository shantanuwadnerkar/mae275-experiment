<!-- Mission file for GADEN preprocessing
    ROS - KINETIC
-->

<launch>
    <arg name="scenario" default="trial1" />
    
    <node pkg="gaden_preprocessing" type="preprocessing" name="preprocessing" output="screen" >
 
        #Cell size
        <param name="cell_size" value="0.8"/>    
        
        #CAD models of the enviroment (.stl)
        <param name="number_of_models" value="1"/>
        <param name="model_0" value="$(find test_env)/$(arg scenario)/cad_models/Part1_feet.STL"/>
        #ROS_DEBUG("Part1 loaded");
        
        #CAD model of the outlets (.stl)
        <param name="number_of_outlet_models" value="0"/>
        #<param name="outlets_model_1" value="$(find test_env)/$(arg scenario)/cad_models/outlet.STL"/>
        #ROS_DEBUG("outlet loaded");

        #3D Location of a point in free-space
        <param name="empty_point_x" value="7.0"/>      ### (m)
        <param name="empty_point_y" value="7.0"/>      ### (m)
        <param name="empty_point_z" value="10"/>      ### (m)
        ROS_DEBUG("free-space loaded");    

        #Wind Data (the node will append _i.csv to the name that is specified here)
        <param name="wind_files" value="$(find test_env)/$(arg scenario)/wind_simulations/dynamic/wind"/>

        #Where to write the output files
        <param name="output_path" value="$(find test_env)/$(arg scenario)" />
    </node>
</launch>

